{
  "name": "fdate",
  "description": "Light-weight, faster datetime formatter for modern browsers",
  "license": "MIT",
  "author": "3846masa <3846masahiro+git@gmail.com>",
  "repository": "git@github.com:3846masa/fdate.git",
  "version": "0.0.4",
  "main": "lib/index",
  "files": [
    "dist",
    "lib"
  ],
  "scripts": {
    "build": "npm-run-all -s clean build:tsc build:rename build:rollup",
    "build:rename": "renamer --find \".js\" --replace \".mjs\" \"./lib/**.js\"",
    "build:rollup": "rollup -c",
    "build:tsc": "tsc",
    "clean": "rimraf lib dist",
    "prepublish": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.3",
    "renamer": "^1.0.0",
    "rimraf": "^2.6.2",
    "rollup": "^0.66.0",
    "rollup-plugin-gzip": "^2.0.0",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-terser": "^3.0.0",
    "typescript": "^3.0.3"
  },
  "keywords": [
    "date",
    "datetime",
    "format",
    "formatter"
  ],
  "jsdelivr": "dist/index.min.js",
  "module": "lib/index.mjs",
  "types": "lib/index.d.ts",
  "unpkg": "dist/index.min.js"
}
